@using Modules.Resources
<div class="navbar navbar-inverse">
    <div class="navbar-brand">@Presentation.News</div>
    <div class="btn btn-default navbar-btn" ng-click="add()">@Presentation.Add</div>
    <div class="btn btn-default navbar-btn" ng-disabled="selectedButton == null" ng-click="edit(selectedButton)">@Presentation.Edit</div>
    <div class="btn btn-default navbar-btn" ng-disabled="selectedButton == null" ng-click="delete(selectedButton)">@Presentation.Delete</div>
</div>
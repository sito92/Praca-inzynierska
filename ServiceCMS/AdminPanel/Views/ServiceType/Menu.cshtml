@{
    Layout = null;
}
@using Modules.Resources
<div class="navbar navbar-inverse">
    <div class="navbar-brand">@Presentation.News</div>
    <div class="btn btn-default navbar-btn" ng-click="add()">@Presentation.Add</div>
    <div class="btn btn-default navbar-btn" ng-disabled="selectedType == null" ng-click="edit(selectedType)">@Presentation.Edit</div>
    <div class="btn btn-default navbar-btn" ng-disabled="selectedType == null" ng-click="delete(selectedType)">@Presentation.Delete</div>
</div>
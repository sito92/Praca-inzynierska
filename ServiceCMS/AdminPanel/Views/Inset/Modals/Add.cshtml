@using Modules.Resources
@{
    ViewBag.Title = "Add";
}
@Scripts.Render("~/bundles/insetModule")
<div class="modal-header">
    Wstawka!!!!
    <div class="row text-center">
        <h2 class="text-modal-heading">@Presentation.Add</h2>
    </div>
</div>
<div class="modal-body">
    <span ng-bind="insetString()"></span>
    <div class="form-group form-field row">
        <div class="col-md-3"><label class="control-label">@Presentation.InsetType</label>
        </div>
        <div class="col-md-9"><select ng-model="choosedInset" ng-options="inset.LocalizedName for inset in avaiableInsets" ng-change="changeInsetType()" class="form-control"></select>
        </div>
    </div>
    <div id="insetPartContainer" class="animate-show" ng-hide="processing"></div>
</div>
<div class="modal-footer">
    <button class="btn btn-primary" ng-click="send()">@Presentation.Add</button>
    <button class="btn btn-default" ng-click="cancel()">@Presentation.Cancel</button>
</div>
